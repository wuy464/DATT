<header class="c-header">
  <div class="l-container">
    <nav class="c-header__nav">
      <div class="c-header__left">
        <ion-icon name="grid-outline"></ion-icon>
        <span class="c-header__logo">Listeners Playlist</span>
       
      </div>
      <ul class="c-header__right">
        <div class="c-header__miniplay">
          <div class="c-header__info">
            <div class="c-header__thumb">
              <img src="https://i1.sndcdn.com/artworks-000167527289-p3zpfg-large.jpg" alt="">
            </div>
            <div class="c-header__name">
              <span class="c-header__song">Firday Comes</span>
              <span class="c-header__author">Early</span>
            </div>
          </div>
          <div class="c-header__button">
            <div class="c-header__btnleft">
              <span class="c-header__prev"><ion-icon name="play-skip-back-outline"></ion-icon></span>
              <span class="c-header__play"><ion-icon name="play-outline"></ion-icon></span>
              <span class="c-header__pause"><ion-icon name="pause-outline"></ion-icon></span>
            </div>
            <div class="c-header__btnright">
              <span class="c-header__next"><ion-icon name="play-skip-forward-outline"></ion-icon></span>
              <span class="c-header__bars"><ion-icon name="menu-outline"></ion-icon></span>
            </div>
          </div>
        </div>
      </ul>
    </nav>
    
  </div>
  
 
</header>

